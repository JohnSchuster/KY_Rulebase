          </otherwise>
        </conclude>
      </rule-xml>
    </rule>
    <rule id="2" name="the employment is a duplicate employment period" ruleburst="true" concludes="b14" folder="0" source-offset="2883">
      <rule-xml>
        <conclude attr-id="b14" entity-id="employment" scope-id="employment0">
          <value>
            <true-value />
          </value>
          <condition>
            <for>
              <relationship relationship-id="employmenttoemployerinstance" target-entity-id="employer" />
              <exists scope-id="the other employment">
                <relationship relationship-id="allemployerinstancesofemployment" target-entity-id="employment" />
                <and>
                  <equals>
                    <scope-for scope-ref="employment0" target-entity-id="employment">
                      <attribute attr-id="p1" />
                    </scope-for>
                    <attribute attr-id="p1" />
                  </equals>
                  <equals>
                    <scope-for scope-ref="employment0" target-entity-id="employment">
                      <attribute attr-id="p2" />
                    </scope-for>
                    <attribute attr-id="p2" />
                  </equals>
                </and>
              </exists>
            </for>
          </condition>
          <otherwise>
            <false-value />
          </otherwise>
        </conclude>
      </rule-xml>
    </rule>
    <rule id="3" name="the most recent employment was for a ten week employer" ruleburst="true" concludes="b12" folder="0" source-offset="3281">
