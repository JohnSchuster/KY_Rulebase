  <rules>
    <rule id="0" name="the employer continues to provide the claimant with part-time employment without interruption to the same extent as was provided from hire" ruleburst="true" concludes="b1" folder="0" source-offset="1607">
      <rule-xml>
        <conclude attr-id="b1" entity-id="employer" scope-id="employer0">
          <value>
            <true-value />
          </value>
          <condition>
            <for-all scope-id="employment0">
              <relationship relationship-id="allemployerinstancesofemployment" target-entity-id="employment" />
              <and>
                <attribute attr-id="b2" />
                <attribute attr-id="b3" />
                <attribute attr-id="b4" />
              </and>
            </for-all>
          </condition>
          <otherwise>
            <false-value />
          </otherwise>
        </conclude>
      </rule-xml>
    </rule>
    <rule id="1" name="the claimant is considered part of group b" ruleburst="true" concludes="b5" folder="0" source-offset="2320">
      <rule-xml>
        <conclude attr-id="b5" entity-id="global" scope-id="global0">
          <value>
            <true-value />
          </value>
          <condition>
            <or>
              <attribute attr-id="b6" />
              <attribute attr-id="b7" />
              <conclude attr-id="b8" entity-id="global" scope-id="global0">
                <value>
                  <true-value />
                </value>
                <condition>
                  <exists scope-id="employment0">
                    <relationship relationship-id="allinferredinstancesoftheemployment" target-entity-id="employment" />
                    <and>
                      <attribute attr-id="b9" />
                      <attribute attr-id="b10" />
                    </and>
