<?xml version="1.0" encoding="utf-8"?>
<test-script product-version="10.4.1.60" schema-version="4">
  <test-case-set>
    <test-case name="TC001-LegalAlien" notes="">
      <ent-set entity-type="global" entity-text="Global">
        <ent-inst id="global 1">
          <attr-inst name="dci_reason_legal_alien" text="dci ruling, claimant is legal alien, reason" type="text">
            <val>
              <text-val>Section 203(a)(7) INA</text-val>
            </val>
          </attr-inst>
          <attr-inst name="claimant_legal_resident" text="the claimant is currently a legal resident for purposes of Kentucky unemployment insurance" type="boolean">
            <expected-val>
              <boolean-val>true</boolean-val>
            </expected-val>
          </attr-inst>
          <attr-inst name="dci_ruling_legal_resident" text="claimant is a legal resident, per dci ruling" type="boolean">
            <expected-val>
              <boolean-val>true</boolean-val>
            </expected-val>
          </attr-inst>
        </ent-inst>
      </ent-set>
    </test-case>
    <test-case name="TC002-Fail" notes="">
      <ent-set entity-type="global" entity-text="Global">
        <ent-inst id="global 1">
          <attr-inst name="claimant_legal_resident" text="the claimant is currently a legal resident for purposes of Kentucky unemployment insurance" type="boolean">
            <expected-val>
              <boolean-val>false</boolean-val>
            </expected-val>
          </attr-inst>
          <attr-inst name="dci_ruling_legal_resident" text="claimant is a legal resident, per dci ruling" type="boolean">
            <val>
              <boolean-val>true</boolean-val>
            </val>
          </attr-inst>
        </ent-inst>
      </ent-set>
    </test-case>
  </test-case-set>
  <outcome-set>
    <outcome name="claimant_legal_resident" display-name="claimant_legal_resident" text="the claimant is currently a legal resident for purposes of Kentucky unemployment insurance" type="boolean" entity-type="global" entity-text="Global">
      <default-change-points>
        <unknown-val />
      </default-change-points>
    </outcome>
    <outcome name="dci_ruling_legal_resident" display-name="dci_ruling_legal_resident" text="claimant is a legal resident, per dci ruling" type="boolean" entity-type="global" entity-text="Global">
      <default-change-points>
        <unknown-val />
      </default-change-points>
    </outcome>
  </outcome-set>
</test-script>