body { 
	font-family: Calibri, Tahoma, Arial, sans-serif;
    font-size: 18px;
}
 i ,b ,p {
    font-size: 18px; 
	margin-left: 5em;	
}
 h1, h2, h3, .heading1, .heading2, .heading3 {
   	font-family: Calibri, Tahoma, Arial, sans-serif;
    color: #003D6E;
	padding-top: 7px;
 	padding-left: 3em;
	Margin-bottom: 1em;
}
ul li {
    padding: 2px 0 0 10px;
    list-style: none; 
    list-style-position: inside;
	margin-left: 3em;	
}
img.center { 
    display: block;   
	margin-left: auto;   
	margin-right: auto; 
}
.note {
     color: #000;
     text-align:center;
     border: 2px solid #003D6E;
     background-color: #D2D2D2;
     margin: 1em 5em 1em 5em;
     padding:15px;
}
.finalnote {
     text-align:center; 
     color: #000;
     border: 2px solid #C00000;
     background-color: #D2D2D2;
     margin: 1em 5em 1em 5em;
     padding:15px;
}
